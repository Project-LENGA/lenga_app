<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@tutor) do |f| %>

      <%= f.label :graduate_year %>
      <%= f.date_select :graduate_year, class: 'form-control', discard_day: true %>

      <%= f.submit 'Register as Tutor!', class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
